\"justify-content-center pb-4\">\n      <b-col cols=\"12\" md=\"6\">\n        <template>\n          <div>\n            <b-form>\n              <b-form-group>\n                <b-form-input\n                  id=\"input-1\"\n                  type=\"text\"\n                  placeholder=\"Ваше имя...\"\n                  v-model=\"name\"\n                ></b-form-input>\n              </b-form-group>\n              <b-form-group>\n                <b-form-input\n                  id=\"input-1\"\n                  type=\"email\"\n                  placeholder=\"Ваш email...\"\n                  v-model=\"email\"\n                ></b-form-input>\n              </b-form-group>\n              <b-form-textarea\n                id=\"textarea-no-resize\"\n                placeholder=\"Ваши вопросы...\"\n                rows=\"3\"\n                no-resize\n                v-model=\"text\"\n              ></b-form-textarea>\n              <div class=\"mt-3\">\n                <b-button block variant=\"secondary\" v-on:click=\"sendMess\"\n                  >Отправить</b-button\n                >\n              </div>\n            </b-form>\n          </div>\n        </template>\n      </b-col>\n    </b-row>\n  </b-container>\n</template>\n\n<script>\nimport Vue from \"vue\";\nimport VueAxios from \"vue-axios\";\nimport axios from \"axios\";\n\nVue.use(VueAxios, axios);\nexport default {\n  data() {\n    return {\n      name: null,\n      email: null,\n      text: null,\n    };\n  },\n\n  methods: {\n    async sendMess() {\n      this.axios.post(\"http://localhost:3000/api/email/send\", {\n        email: this.email,\n        text: this.text,\n        name: this.name,\n      });\n    },\n  },\n};\n</script>\n\n<style>\n</style>"],"sourceRoot":"src/components"}]}